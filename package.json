{
	"name": "@lxghtless/liscio-undici",
	"version": "1.2.1",
	"description": "A simple undici http client wrapper with interceptors.",
	"main": "lib/index.js",
	"types": "lib/index.d.ts",
	"files": [
		"lib"
	],
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"lint": "eslint --fix \"lib/**/*.js\" \"test/**/*.js\"",
		"pretest": "eslint \"lib/**/*.js\" \"test/**/*.js\"",
		"test": "BROKKR_LOG_LEVEL=silent tap test/*.test.js test/**/*.test.js",
		"coverage": "tap --coverage-report=text-lcov | codecov --pipe --disable=gcov",
		"test:types": "tsd",
		"clean": "rimraf .nyc_output",
		"install-husky": "husky install"
	},
	"keywords": [],
	"author": "lxghtless",
	"license": "ISC",
	"dependencies": {
		"@lxghtless/brokkr": "^0.1.0",
		"undici": "^3.3.3"
	},
	"devDependencies": {
		"@lxghtless/eslint-config-prettier": "^1.1.0",
		"@types/node": "^14.14.34",
		"codecov": "^3.8.1",
		"husky": "^5.1.3",
		"rimraf": "^3.0.2",
		"sinon": "^9.2.4",
		"tap": "^14.11.0",
		"tsd": "^0.14.0"
	},
	"husky": {
		"hooks": {
			"pre-commit": "npm run lint"
		}
	},
	"tsd": {
		"directory": "test/types",
		"compilerOptions": {
			"esModuleInterop": true
		}
	}
}
